{
    "manifest_version": 3,
    "name": "Draggable To-Do App",
    "version": "1.0",
    "description": "A draggable To-Do app with persistent storage.",
    "permissions": ["storage", "windows", "tabs" ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
